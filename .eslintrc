{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      1,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "quote-props": [
      1,
      "consistent-as-needed"
    ],
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "radix": 0,
    "space-infix-ops": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "default-case": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "quotes": 0,
    "indent": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "space-before-blocks": 0,
    "prefer-template": 0,
    "camelcase": 0,
    "prefer-default-export": 0,
    "padded-blocks": 0,
    "spaced-comment": 0,
    "arrow-body-style": 0
  }
}
